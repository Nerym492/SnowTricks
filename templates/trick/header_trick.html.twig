<h1>{{ trick.name }}</h1>
<header class="head-trick-details">
    {% if headerImage is not empty %}
        <img src="{{ path(
            'get_trick_image',
            {'slug': trick.slug,'imageName': headerImage.fileName}
        ) }}" alt="image {{ trickName }}" id="image-header-details">
    {% else %}
        <img src="/build/images/image-placeholder.webp" alt="Image placeholder" id="image-header-details">
    {% endif %}
</header>